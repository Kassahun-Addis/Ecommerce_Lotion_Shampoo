e } = await instance.requestPaymentMethod();\n      const { data } = await axios.post(\"/api/v1/Product/braintree/payment\", {\n        nonce,\n        cart,\n      });\n      setLoading(false);\n      localStorage.removeItem(\"cart\");\n      setCart([]);\n      navigate(\"/dashboard/user/orders\");\n      toast.success(\"Payment Completed Successfully \");\n    } catch (error) {\n      console.log(error);\n      setLoading(false);\n    }\n  };\n  return (\n    <Layout>\n      <div className=\" cart-page\">\n        <div className=\"row\">\n          <div className=\"col-md-12\">\n            <h1 className=\"text-center bg-light p-2 mb-1\">\n              {!auth?.user\n                ? \"Hello Guest\"\n                : `Hello  ${auth?.token && auth?.user?.name}`}\n              <p className=\"text-center\">\n                {cart?.length\n                  ? `You Have ${cart.length} items in your cart ${\n                      auth?.token ? \"\" : \"please login to checkout !\"\n                    }`\n                  : \" Your Cart Is Empty\"}\n              </p>\n            </h1>\n          </div>\n        </div>\n        <div className=\"container \">\n          <div className=\"row \">\n            <div className=\"col-md-7  p-0 m-0\">\n              {cart?.map((p) => (\n                <div className=\"row card flex-row\" key={p._id}>\n                  <div className=\"col-md-4\">\n                    <img\n                      src={`/api/v1/Product/productPhoto/${p._id}`}\n                      className=\"card-img-top\"\n                      alt={p.name}\n                      width=\"100%\"\n                      height={\"130px\"}\n                    />\n                  </div>\n                  <div className=\"col-md-4\">\n                    <p>{p.name}</p>\n                    <p>{p.description.substring(0, 30)}</p>\n                    <p> {t(\"cart:Price\")} : {p.price}</p>\n                  </div>\n                  <div className=\"col-md-4 cart-remove-btn\">\n                    <button\n                      className=\"btn btn-danger\"\n                      onClick={() => removeCartItem(p._id)}\n                    >\n                      {t(\"cart:Remove\")}\n                    </button>\n                  </div>\n                </div>\n              ))}\n            </div>\n            <div className=\"col-md-5 cart-summary \">\n              <h2>DropShipping Cart Summary</h2>\n              <p>{t(\"cart:TotalCheckoutPayment\")}</p>\n              <hr />\n              <h4>{t(\"cart:total\")} : {totalPrice()} </h4>\n              {auth?.user?.address ? (\n                <>\n                  <div className=\"mb-3\">\n                    <h4>{t(\"cart:CurrentAddress\")}</h4>\n                    <h5>{auth?.user?.address}</h5>\n                    <button\n                      className=\"btn btn-outline-warning\"\n                      onClick={() => navigate(\"/dashboard/user/profile\")}\n                    >\n                      {t(\"cart:UpdateAddress\")}\n                    </button>\n                  </div>\n                </>\n              ) : (\n                <div className=\"mb-3\">\n                  {auth?.token ? (\n                    <button\n                      className=\"btn btn-outline-warning\"\n                      onClick={() => navigate(\"/dashboard/user/profile\")}\n                    >\n                      \n                      {t(\"cart:updateAddress\")}\n                    </button>\n                  ) : (\n                    <button\n                      className=\"btn btn-outline-warning\"\n                      onClick={() =>\n                        navigate(\"/login\", {\n                          state: \"/cart\",\n                        })\n                      }\n                    >\n                     {t(\"cart:Login\")}\n                    </button>\n                  )}\n                </div>\n              )}\n           